<?xml version="1.0" encoding="UTF-8"?>
<%
  @imports = { control: { Button: nil }, layout: { StackPane: nil } }

  def translate_imports
    ans = []
    @imports.each do |a, b|
      b.each do |c|
        ans << "<?import javafx.scene.#{a}.#{c}?>"
      end
    end
    ans.join("\n")
  end
%>
<%= translate_imports %>
